{
  "name": "react-typescript-template",
  "version": "0.0.1",
  "description": "A template for a React Typescript project",
  "author": "me@danhle.dev",
  "license": "UNLICENSED",
  "keywords": [
    "Typescript",
    "React",
    "Redux",
    "Redux-Saga"
  ],
  "dependencies": {
    "axios": "^0.26.1",
    "axios-mock-adapter": "^1.20.0",
    "connected-react-router": "^6.9.2",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "react-scripts": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.11",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prepare": "rimraf .husky&&husky install&&husky add .husky/pre-commit \"npm run precommit\"",
    "precommit": "lint-staged",
    "lint": "npm run eslint",
    "lint-fix": "npm run eslint-fix",
    "eslint": "eslint . --ext .jsx,.js,.tsx,.ts",
    "eslint-fix": "eslint . --ext .jsx,.js,.tsx,.ts --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  }
}
